{
  "name": "kevinjlope.github.io",
  "version": "1.0.0",
  "description": "You can use the [editor on GitHub](https://github.com/kevjlope/kevjlope.github.io/edit/master/README.md) to maintain and preview the content for your website in Markdown files.",
  "source": "src/index.html",
  "homepage": "https://kevinjlope.github.io/",
  "scripts": {
    "start": "parcel",
    "build": "parcel build",
    "build-gh": "parcel build --public-url 'https://kevinjlope.github.io/'",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --quiet",
    "predeploy": "npm run build-gh",
    "deploy": "gh-pages -d dist"
  },
  "author": "Kevin Lopez",
  "license": "ISC",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.7",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gh-pages": "^4.0.0",
    "html-webpack-plugin": "^5.5.0",
    "parcel": "^2.7.0",
    "prettier": "^2.7.1",
    "process": "^0.11.10",
    "typescript": "^4.8.4"
  }
